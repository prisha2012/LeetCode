class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        Set<Integer> st1=new HashSet<>();
         for (int num : nums1) st1.add(num);
        Set<Integer> st2=new HashSet<>();
         for (int num : nums2) st2.add(num);

        st1.retainAll(st2);

        int res[]=new int[st1.size()];
        int i=0;
        for(int nums:st1){
            res[i++]=nums;
        }
        return res;
    }
}